#new_environment
  .page_header
    %h1
      Setup a New Environment

  .left_column
    - form_for(@environment) do |f|
      = f.error_messages
      = render :partial => "form", :object => f
      = f.submit 'Create', :class => "submit"
      = link_to 'Cancel', environments_path, :class => "actionlink"
  .right_column
    = render :partial=>'form_new_help'
