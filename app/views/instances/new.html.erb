<h1>New instance</h1>

<% form_for(@instance) do |f| %>
  <%#= f.error_messages %>

  <p>
    <%= f.label :image_id %><br />
    <%= f.text_field :image_id %>
  </p>
  <p>
    <%= f.label :key_pair_name %><br />
    <%= f.text_field :key_pair_name %>
  </p>
  <p>
    <%= f.submit 'Create' %>
  </p>
<% end %>

<%= link_to 'Back', instances_path %>
